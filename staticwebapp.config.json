{
	"routes": [
		{
			"route": "/records*",
			"allowedRoles": ["authenticated"]
		},
		{
			"route": "/orcs*",
			"allowedRoles": ["authenticated"]
		},
		{
			"route": "/charts*",
			"allowedRoles": ["authenticated"]
		},
		{
			"route": "/livecam*",
			"allowedRoles": ["authenticated"]
		},
		{
			"route": "/settings",
			"allowedRoles": ["admin"]
		},
		{
			"route": "/api/*",
			"allowedRoles": ["authenticated"]
		},
		{
			"route": "/login",
			"rewrite": "/.auth/login/github"
		},
		{
			"route": "/loginms",
			"rewrite": "/.auth/login/aad"
		},
		{
			"route": "/.auth/login/twitter",
			"statusCode": 404
		},
		{
			"route": "/logout",
			"redirect": "/.auth/logout"
		}
	],
	"navigationFallback": {
		"rewrite": "index.html",
		"exclude": ["/assets/*.{png,jpg,jpeg,gif,bmp}", "/static/css/*"]
	},
	"mimeTypes": {
		".json": "text/json"
	},
	"responseOverrides": {
		"400": {
			"rewrite": "/invalid-invitation-error.html"
		}
	}
}
